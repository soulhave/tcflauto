									<xs:restriction base="xs:string">
										<xs:enumeration value="I"/>
										<xs:enumeration value="N"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ExceptionCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Codigo de erro em caso de falha.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ExceptionDescription" type="xs:string">
								<xs:annotation>
									<xs:documentation>Mensagem de retorno do processo de sincronismo em caso de erros.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LegacyAck">
		<xs:annotation>
			<xs:documentation>RETORNO Legado: Solicitação de Faturamento/Estorno ao TCFL [confirmação do retorno ao TCFL ]</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="returnStage">
					<xs:annotation>
						<xs:documentation>Retorno de qual estágio o legado está respondendo. Ex: TCFL, para o 1° estágio; SYNCHRO para o 2°estágio e ORACLE para o 3° estágio.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="TCFL"/>
							<xs:enumeration value="SYNCHRO"/>
							<xs:enumeration value="ORACLE"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="messageID" type="xs:string">
					<xs:annotation>
						<xs:documentation>atributo identificado na integração e persistidos nos legados para identificação do processo de integração</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="identifySystem" type="xs:string">
					<xs:annotation>
						<xs:documentation>Identificador do Sistema</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="provisoryNumberNF" type="xs:string">
					<xs:annotation>
						<xs:documentation>Número provisório NF Sistema Origem</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="requestNumberTCFL" type="xs:string">
					<xs:annotation>
						<xs:documentation>O número da solicitação no TCFL</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="processDate" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Data do processamento</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="statusProcess">
					<xs:annotation>
						<xs:documentation>Status do processamento</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="ABORTADA"/>
							<xs:enumeration value="ERRO"/>
							<xs:enumeration value="SUCESSO"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Exception" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Exceção</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ExceptionType">
								<xs:annotation>
									<xs:documentation>Retornado somente em caso de exceção. I (Infra-Estrutura) ou N(Negocio)</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="I"/>
										<xs:enumeration value="N"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ExceptionCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Codigo de erro em caso de falha.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ExceptionDescription" type="xs:string">
								<xs:annotation>
									<xs:documentation>Mensagem de retorno do processo de sincronismo em caso de erros.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
