<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:ns1="http://www.vale.com/CM/UnifComercial/LogisticBilling/Schema"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="2.0"
	targetNamespace="http://www.vale.com/CM/UnifComercial/LogisticBilling/Schema"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
        <xs:appinfo>
            <jxb:schemaBindings>
                <jxb:package name="br.com.cit.jaxb.generate"/>
                <jxb:nameXmlTransform>
                    <jxb:elementName suffix="Element"/>
                </jxb:nameXmlTransform>
            </jxb:schemaBindings>
        </xs:appinfo>
    </xs:annotation>
	<xs:element name="LogisticBilling">
		<xs:annotation>
			<xs:documentation>Faturamento Logístico</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="ns1:RequestBilling"/>
				<xs:element ref="ns1:CompositeRequestBilling"/>
				<xs:element ref="ns1:ResponseTCFL"/>
				<xs:element ref="ns1:LegacyAck"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompositeRequestBilling">
	       <xs:annotation>
            <xs:documentation>Solicitação de Faturamento Logístico Composto</xs:documentation>
        </xs:annotation>
	        <xs:complexType>
	           <xs:sequence>
	                <xs:element name="requestBillingCTE">
				        <xs:complexType>
				          <xs:sequence>
				            <xs:element ref="ns1:RequestBilling"/>
				          </xs:sequence>
                       </xs:complexType>
                   </xs:element>                    
                   <xs:element name="requestBillingCTMC">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element ref="ns1:RequestBilling"/>
                          </xs:sequence>
                       </xs:complexType>
                   </xs:element>
		   </xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RequestBilling">
		<xs:annotation>
			<xs:documentation>Solicitação de Faturamento</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="HeaderRequestBilling">
					<xs:annotation>
						<xs:documentation>Informações que definem a solicitação</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="messageID" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>atributo identificado na integração e persistidos nos legados para identificação do processo de integração</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="identifySystem" type="xs:string">
								<xs:annotation>
									<xs:documentation>Identificador do Sistema</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="provisoryNumberNF" type="xs:string">
								<xs:annotation>
									<xs:documentation>Número provisório NF Sistema Origem</xs:documentation>
								</xs:annotation>
							</xs:element>
							
							<xs:element name="requestNumberTCFL" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Utilizado para referenciar uma solicitação a ser excluída</xs:documentation>
								</xs:annotation>
							</xs:element>

							<xs:element name="requestType" minOccurs="1" maxOccurs="1" type="xs:string">
								<xs:annotation>
									<xs:documentation>Tipo de solicitação, descreve o tipo de NF</xs:documentation>
								</xs:annotation>
							</xs:element>
							
							<xs:element name="actionCode">
								<xs:annotation>
									<xs:documentation>Código da ação, INCLUSAO = Solicitar faturamento, CANCELAMENTO= estorno de faturamento</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="INCLUSAO"/>
										<xs:enumeration value="CANCELAMENTO"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="emitterLocalCode">
								<xs:annotation>
									<xs:documentation>Será utilizado o código numérico da ferrovia.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="natOperationCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código da Natureza da operação</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="numberNF" type="xs:string" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Número da NF</xs:documentation>
                                </xs:annotation>
                            </xs:element>
							<xs:element name="serieSubserieFiscalDoc" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>No caso de uma Solicitação d</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="originCteKey" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Chave do CTe</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="44"/>
										<xs:maxLength value="44"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="transportViaCode" type="xs:string"/>
							<xs:element name="calcTaxSynchroFlag" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Indicador do cálculo de imposto Synchro.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="S"/>
										<xs:enumeration value="N"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="calcDutyFlag" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Indicador de encargo calculado, o campo deve sempre ter valor _S_.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="S"/>
										<xs:enumeration value="N"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="dutyCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código do encargo</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="dutyValue" type="xs:double" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Valor do encargo</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sourceCodeIBGE" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Codigo IBGE Origem.  Obrigatorio para ICMS e ISS</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="destCodeIBGE" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Codigo IBGE destino. Obrigatorio somente para ICMS</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PanelNotificationFlag">
								<xs:annotation>
									<xs:documentation>1 - O Painel devera ser notificado / 0 - O Painel não devera ser notificado</xs:documentation>							
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="1"/>
										<xs:enumeration value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ERPCode" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código de integração ERP</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="3"></xs:minLength>
										<xs:maxLength value="3"></xs:maxLength>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="RequestPerson">
					<xs:annotation>
						<xs:documentation>Solicitação Pessoas</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="emitterPersonCode" type="xs:string">
								<xs:annotation>
									<xs:documentation>Código da Pessoa emitente</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="remitterPersonCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código da pessoa remetente</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="destinationPersonCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código da pessoa destinatária</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="consigneePersonCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código da empresa consignatária</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="expediterPersonCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código da empresa expedidora</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="receiverPersonCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código da empresa recebedora</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="OtherDetails" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Outras Informações</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="paymentConditionCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código de condição de pagamento</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="conditionPaymentDateRef" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Data Referência de condição de pagamento</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="itensQuantity" type="xs:integer">
								<xs:annotation>
									<xs:documentation>Campo numérico de 3 dígitos. Ex.: 001Deve ser igual ao numero de tags RequestItens.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="textMessage" type="xs:string" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="EspecificAtributes">
					<xs:annotation>
						<xs:documentation>Atributos Específicos </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="replacementTaxCNPJ" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>CNPJ de substituição tributária</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="freightresponsibilityCode" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código de responsabilidade de frete</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="currencyCode" type="xs:string">
								<xs:annotation>
									<xs:documentation>Código Moeda</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="companyUserResponsible" type="xs:string">
								<xs:annotation>
									<xs:documentation>Empresa usuário responsável</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="userRegistrationResponsible" type="xs:string">
								<xs:annotation>
									<xs:documentation>Matricula do usuário responsável</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="codeTaxIncidence" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código de incidência de imposto. Utilizar quando faturar doc. Auxiliares (ver Documentos Auxiliares)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="needCalcSynchroFlag" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Indicador de necessidade de cálculo Synchro</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="S"/>
										<xs:enumeration value="N"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="AssociatedDocuments" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Documentos associados. Só é preenchido no caso de despachos contingenciais</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="emitterDateDof" type="xs:dateTime">
								<xs:annotation>
									<xs:documentation>Data e hora de emissão do Dof  associado.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="especiesCodeDof" type="xs:string">
								<xs:annotation>
									<xs:documentation>Código espécie Dof  Associado, fila de impressão do synchro</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="emitterPersonCodeDof" type="xs:string">
								<xs:annotation>
									<xs:documentation>Código pessoa emitente Dof associado, deve conter o CNPJ sem caracteres "/", ".".</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="SeriesSubseriesDof" type="xs:string">
								<xs:annotation>
									<xs:documentation>Serie Subserie Dof Associado</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="numberDof" type="xs:string">
								<xs:annotation>
									<xs:documentation>Numero Dof Associado</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="totalValueDof" type="xs:double">
								<xs:annotation>
									<xs:documentation>Valor total Dof Associado</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Complements">
					<xs:annotation>
						<xs:documentation>Complementos</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="businessUnitCode" type="xs:string">
								<xs:annotation>
									<xs:documentation>No caso de Ferrovia o campo deve sempre ter o valor _F_. No caso de Porto o campo deve sempre ter o valor _P_. No caso de Navegação o campo deve sempre ter o valor _N_. Código do ELO.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="businessSubUnitCode" type="xs:string">
								<xs:annotation>
									<xs:documentation>1.	No caso da FCA o campo deve sempre ter o valor _A_. Código do ELO.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="serviceConditionCode" type="xs:string">
								<xs:annotation>
									<xs:documentation>Código do ELO.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="especiesCodeDof" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Código do Synchro. Só é enviado no caso de faturamento</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ReferenceDate" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Data da realização do serviço ou venda do produto.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="RequestItem" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Solicitacao de Item. Nao é enviado se for cancelamento.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="numberSeq" type="xs:integer">
								<xs:annotation>
									<xs:documentation>Atributo da tag ItemSolicitacao.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="RequestDetailType" type="xs:integer">
								<xs:annotation>
									<xs:documentation>Tipo detalhe Item solicitacao No caso de Porto o campo deve conter o valor _1_. No caso de Passageiro o campo deve conter o valor _2_.No caso de Ferrovia o campo deve conter o valor _3_.Para faturamento de doc. auxiliar utilizar o valor _4_.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="aditionalText" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Texto Complementar</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="itensQuantity" type="xs:double">
								<xs:annotation>
									<xs:documentation>Quantidade de mercadoria transportada</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="totalValue" type="xs:double">
								<xs:annotation>
									<xs:documentation>Valor Total. Deve ser equivalente a soma dos campos ValorSemImposto de todos os ComponentePreco desse item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="valueICMS" type="xs:double" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Valor do ICMS</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="unitPrice" type="xs:double">
								<xs:annotation>
									<xs:documentation>Preco Unitario</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="measurementUnit" type="xs:string">
								<xs:annotation>
									<xs:documentation>Unidade de medida. Se não for informado enviar o valor U</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="NopItem" type="xs:string">
								<xs:annotation>
									<xs:documentation>Código do Synchro</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="dutyValue" type="xs:double" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Valor encargo item</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sourceCodeIBGE" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Codigo IBGE Origem.  Obrigatorio para ICMS e ISS</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="destCodeIBGE" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Codigo IBGE destino. Obrigatorio somente para ICMS</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="dispatchSeries" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Série do Despacho. Utilizar quando se tratar de uma Solicitacao de Faturamento de Despacho, no caso de Documento Aux nao enviar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="dispatchNumber" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Número Despacho. Utilizar quando se tratar de uma Solicitacao de Faturamento de Despacho, no caso de Documento Aux nao enviar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="dispatchVersion" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Versão do despacho. Utilizar quando se tratar de uma Solicitacao de Faturamento de Despacho, no caso de Documento Aux nao enviar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="dispatchDate" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Data do despacho. Utilizar quando se tratar de uma Solicitacao de Faturamento de Despacho, no caso de Documento Aux nao enviar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="stationProvenience" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Procedëncia Estacao. Utilizar quando se tratar de uma Solicitacao de Faturamento de Despacho, no caso de Documento Aux nao enviar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="stationDest" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Estação destino. Utilizar quando se tratar de uma Solicitacao de Faturamento de Despacho, no caso de Documento Aux nao enviar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="carriedWare" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Mercadoria transportada</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="originalDispatchNumber" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Numero despacho original.Concatenação de série + número + versão do despacho anterior. É utilizado em Notas Fiscais Complementares, Notas de Crédito, Correções Fiscais e solicitações de tráfego mútuo não faturável.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ValueISS" type="xs:double" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Valor ISS Item</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="componentsQuantity" type="xs:integer">
								<xs:annotation>
									<xs:documentation>Quantidade de Componentes</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="auxDocNumber" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Numero do documento auxiliar. Utilizar quando se tratar de um Documento Aux, no caso de Faturamento de despacho nao enviar.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ComponentPrice" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Preço Componente</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="SeqRequestItemComp" type="xs:integer">
											<xs:annotation>
												<xs:documentation>Numero Sequencial componente item solicitação.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="componentType" type="xs:string">
											<xs:annotation>
												<xs:documentation>Tipo de componente. No caso de Frete esse campo deve ter valor "P" . No caso de Serviço/Seguro esse campo deve ter o valor "S".</xs:documentation>
